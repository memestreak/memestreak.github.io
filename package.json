{
  "name": "memestreak.github.io",
  "version": "1.0.0",
  "main": "src/eleventy.config.js",
  "scripts": {
    "build:sass": "sass src/style:_site/style",
    "build:eleventy": "eleventy --config=src/eleventy.config.js",
    "watch:sass": "sass --watch src/style:_site/style",
    "watch:eleventy": "eleventy --serve --config=src/eleventy.config.js",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "keywords": [
    "eminor.net",
    "site",
    "eleventy",
    "11ty"
  ],
  "author": "",
  "license": "ISC",
  "description": "Site source for eminor.net",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.86.0"
  },
  "devDependencies": {
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.0",
    "markdown-it-footnote": "^4.0.0"
  }
}
